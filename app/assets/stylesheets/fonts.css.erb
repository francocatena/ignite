@font-face {
  font-family: "Reenie Beanie";
  src: url('<%= asset_path 'ReenieBeanie.eot' %>');
  src: url('<%= asset_path 'ReenieBeanie.eot' %>?#iefix') format('embedded-opentype'),
       url('<%= asset_path 'ReenieBeanie.woff' %>') format('woff'),
       url('<%= asset_path 'ReenieBeanie.ttf' %>') format('truetype'),
       url('<%= asset_path 'ReenieBeanie.svg' %>#ReenieBeanie') format('svg');
  font-style: normal;
  font-weight: normal;
}
