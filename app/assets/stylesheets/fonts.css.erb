@font-face {
  font-family: "Reenie Beanie";
  src: url('<%= image_path 'fonts/reenie_beanie/ReenieBeanie.eot' %>');
  src: url('<%= image_path 'fonts/reenie_beanie/ReenieBeanie.eot' %>?#iefix') format('embedded-opentype'),
       url('<%= image_path 'fonts/reenie_beanie/ReenieBeanie.woff' %>') format('woff'),
       url('<%= image_path 'fonts/reenie_beanie/ReenieBeanie.ttf' %>') format('truetype'),
       url('<%= image_path 'fonts/reenie_beanie/ReenieBeanie.svg' %>#ReenieBeanie') format('svg');
  font-style: normal;
  font-weight: normal;
}

@font-face {
  font-family: "IconicStroke";
  src: url('<%= image_path 'fonts/iconic_stroke/iconic_stroke.eot' %>');
  src: url('<%= image_path 'fonts/iconic_stroke/iconic_stroke.eot' %>?#iefix') format('embedded-opentype'),
       url('<%= image_path 'fonts/iconic_stroke/iconic_stroke.woff' %>') format('woff'),
       url('<%= image_path 'fonts/iconic_stroke/iconic_stroke.ttf' %>') format('truetype'),
       url('<%= image_path 'fonts/iconic_stroke/iconic_stroke.svg' %>#iconic') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
