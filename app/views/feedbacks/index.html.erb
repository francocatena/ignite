<table class="list">
  <thead>
    <tr>
      <th><%= Feedback.human_attribute_name 'lesson_id' %></th>
      <th><%= Feedback.human_attribute_name 'rate' %></th>
      <th><%= Feedback.human_attribute_name 'ip' %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.lesson %></td>
        <td><%= feedback.rate %></td>
        <td><%= feedback.ip %></td>
        <td><%= link_to_show feedback_path(feedback, details: true) %></td>
        <td><%= link_to_edit edit_feedback_path(feedback) %></td>
        <td><%= link_to_destroy(feedback) if feedback.can_be_destroyed? %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<nav class="links">
  <%= pagination_links(@feedbacks) %>
</nav>